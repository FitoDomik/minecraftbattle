RewriteEngine On
Options -Indexes
<FilesMatch "\.(html|php)$">
    Header set Cache-Control "public, max-age=3600"
</FilesMatch>
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^$ index.html [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^commands/coreprotect/?$ coreprotect_commands.html [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^commands/simpleclans/?$ simpleclans_commands.html [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^download/?$ download.html [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^admin/?$ admin.php [L,QSA]
<Files "config.php">
    Require all denied
</Files>
<Files "database.sql">
    Require all denied
</Files>
<Files ".htaccess">
    Require all denied
</Files>
<Files "*.log">
    Require all denied
</Files>
Header always unset X-Powered-By
Header unset X-Powered-By
<IfModule mod_headers.c>
    Header always set X-Content-Type-Options "nosniff"
    Header always set X-XSS-Protection "1; mode=block"
    Header always set Referrer-Policy "strict-origin-when-cross-origin"
    Header always set Content-Security-Policy "frame-ancestors 'self'; default-src 'self' 'unsafe-inline' 'unsafe-eval' https://fonts.googleapis.com https://fonts.gstatic.com; img-src 'self' data: https:; font-src 'self' https://fonts.gstatic.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; script-src 'self' 'unsafe-inline' 'unsafe-eval'"
    <FilesMatch "\.(css|js)$">
        Header set Cache-Control "public, max-age=2592000"
    </FilesMatch>
    <FilesMatch "\.(png|jpg|jpeg|gif|ico|webp|svg)$">
        Header set Cache-Control "public, max-age=31536000"
        Header always set X-Content-Type-Options "nosniff"
    </FilesMatch>
    <FilesMatch "^favicon.*\.(ico|png)$">
        Header always set X-Content-Type-Options "nosniff"
    </FilesMatch>
    <FilesMatch "\.(woff|woff2|ttf|eot)$">
        Header set Cache-Control "public, max-age=31536000"
        Header set Access-Control-Allow-Origin "*"
        Header always set X-Content-Type-Options "nosniff"
    </FilesMatch>
    <FilesMatch "\.webmanifest$">
        Header set Cache-Control "public, max-age=604800"
        Header always set X-Content-Type-Options "nosniff"
        Header set Content-Type "application/manifest+json; charset=UTF-8"
    </FilesMatch>
</IfModule>
<Files "TLauncher-Installer.exe">
    Header set Content-Type "application/octet-stream"
    Header set Content-Disposition "attachment; filename=TLauncher-Installer.exe"
</Files>
ErrorDocument 403 /403.html
ErrorDocument 404 /404.html